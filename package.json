    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-mocha-test": "^0.12.7",
    "grunt-sass-lint": "^0.2.0",
    "load-grunt-config": "^0.19.2",
    "load-grunt-tasks": "^3.5.0",
    "mocha": "^2.5.3",
