{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for starting react-redux projects",
  "main": "Gruntfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lburdock/react-redux-boilerplate.git"
