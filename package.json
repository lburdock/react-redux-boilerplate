  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --open --config webpack.config.dev.js",
    "sasslint": "./node_modules/.bin/sass-lint -v",
    "eslint": "./node_modules/.bin/eslint src/js/**/*.js src/js/**/*.jsx",
    "lint": "npm run sasslint | npm run eslint",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register tests/**/*.js",
    "compile": "./node_modules/.bin/webpack --config webpack.config.prod.js"
  },
  "keywords": [],
